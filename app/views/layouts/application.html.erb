<!DOCTYPE html>
<html>
    <head>
        <link href="data:image/x-icon;base64," rel="icon" type="image/x-icon" />
        <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
        <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
        <%= csrf_meta_tags %>
    </head>
  
<head>
  <link href="data:image/x-icon;base64," rel="icon" type="image/x-icon" />
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

  
<div class="container">
  <% flash.each do |message_type, message| %>
      <div class="alert alert-<%= message_type %>"><%= message %></div>
  <% end %>
<%= yield %>
</div>

  <div id="menu_tabs" class="container">
    <%= render 'layouts/footer' %>
    </div>
      
    <div id="proyectosAsignados" class="modal fade" role="dialog">
      <div class="modal-dialog">
                <!-- Modal content-->
              <div class="modal-content">
                  
                  <div class="modal-header">
                      <button type="button" class="close" data-dismiss="modal">&times;</button>
                      <h4 class="modal-title"> </h4>
                  </div>
                  
                  <div class="modal-body">
                      <head> Sus proyectos asignados son: </head>
                      <p>
                      <div class="input-group">
                        <% if current_user then %>
                          <% @fisica = Fisica.where("persona_id == :id", {id: current_user.persona_id}).first  %>
                          <% existeRecurso1 = defined?  Recurso.where("fisica_id == :id", {id: @fisica.id}).first %>
                          <% if existeRecurso1 then %>
                            <% @recurso = Recurso.where("fisica_id == :id", {id: @fisica.id}).first  %>
                            <% existeProyecto = defined?  @recurso.proyectos.all %>
                            <% if existeProyecto then %>
                              <% @proyectos = @recurso.proyectos.all %>
                              <% if !@proyectos.empty? then%>
                                <%  @proyectos.each do |proyecto| %>
                                  Nombre: <%= proyecto.nombre %> Estado: <%= proyecto.estado %>  </br>
                                <%end%>
                              <%end%>
                            <% end %>
                          <% end %>
                        <% end %>
                      </div>
                      </p>
                  </div>
                  
                  <div class="modal-footer">
                  </div>
              </div>
          
          </div>
      </div>
      
    <div id="ultimosEntregables" class="modal fade" role="dialog">
      <div class="modal-dialog">
                <!-- Modal content-->
              <div class="modal-content">
                  
                  <div class="modal-header">
                      <button type="button" class="close" data-dismiss="modal">&times;</button>
                      <h4 class="modal-title"> </h4>
                  </div>
                  
                  <div class="modal-body">
                      <head> Sus Ãºltimos entregables son: </head>
                      <p>
                      <div class="input-group">
                        <% if current_user %>
                          <% @fisica = Fisica.where("persona_id == :id", {id: current_user.persona_id}).first  %>
                          <% existeRecurso2 = defined?  Recurso.where("fisica_id == :id", {id: @fisica.id}).first %>
                          <% if existeRecurso2 then %>
                            <% @recurso = Recurso.where("fisica_id == :id", {id: @fisica.id}).first  %>
                            <% existeEntregable = defined?  @recurso.entregables.all %>
                            <% if existeEntregable then %>
                              <% @entregables = @recurso.entregables.all %>
                              <% if !@entregables.empty? then%>
                                <%  @entregables.each do |entregable| %>
                                <% @proyecto = Proyecto.find(entregable.proyecto_id) %>
                                  Descripcion: <%= entregable.descripcion %> Proyecto: <%= @proyecto.nombre %>   </br>
                                <%end%>
                              <%end%>
                           <% end %>
                          <% end %>
                        <% end %>
                      </div>
                      </p>
                  </div>
                  
                  <div class="modal-footer">
                  </div>
              </div>
          
          </div>
      </div>
    
</body>
</html>
